{
    "env": {
        "browser": true
    },
    "extends": "airbnb",
    "rules": {
        "no-underscore-dangle": 0,
        "no-restricted-properties": 0,
        "no-bitwise": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-mixed-operators": 0,
        "prefer-const": 0,
        "indent": ["error", 4]
    }
}
